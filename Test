local Key = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kompotas/ActualLirp/refs/heads/main/Key"))()

if not Key then       
    warn("Key Doesn't Exist. Error: Contact Administrator")
    return
end

local FoundGame = false

local GameIDS = {
    ["Counter Blox"] = {301549746, "https://codeberg.org/csgofirepro7/NewLoader/raw/branch/main/CounterBlox"},
    ["Project Delta"] = {7336302630, "https://codeberg.org/csgofirepro7/NewLoader/raw/branch/main/ProjectDelta"},
    ["Project Delta Border"] = {7353845952, "https://codeberg.org/csgofirepro7/NewLoader/raw/branch/main/ProjectDelta"},
}

for GameName, Info in pairs(GameIDS) do
    local ID = Info[1]
    local Script = Info[2]

    if game.PlaceId == ID then
        DetectedGameLabel.Text = "Game Detected: " .. GameName
        break
    end
end

LoadScriptButton.MouseButton1Click:Connect(function()
    if KeyInputBox.Text ~= Key then
        warn("Key Doesn't Exist Please Try An Valid Key. Error: Key You Provided Is Not Valid")
        return
    else
        getgenv().Key = KeyInputBox.Text
    end
    
    for GameName, Info in pairs(GameIDS) do
        local ID = Info[1]
        local Script = Info[2]

        if game.PlaceId == ID then
            FoundGame = true
            MainFrame:Destroy()
            loadstring(game:HttpGet(Script, true))()
            return
        end
    end
end)    
